					<MenuItem Header="Navigation Pane" IsCheckable="True" IsChecked="True" InputGestureText="Alt + S"/>
				</MenuItem>
			</MenuItem>
			<MenuItem Header="Edit" VerticalAlignment="Top" Foreground="#FF333333" FontFamily="Verdana" FontSize="11" Margin="30,3,0,0">
