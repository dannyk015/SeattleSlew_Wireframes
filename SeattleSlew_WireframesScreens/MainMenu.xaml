					<i:Interaction.Triggers>
						<i:EventTrigger EventName="MouseLeftButtonDown">
							<pi:NavigateToScreenAction TargetScreen="SeattleSlewSketchScreens.ReportWindow"/>
						</i:EventTrigger>
					</i:Interaction.Triggers>
				</MenuItem>
