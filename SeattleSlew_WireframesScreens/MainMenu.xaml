	mc:Ignorable="d"
	x:Class="SeattleSlewSketchScreens.MainWindow"
	Height="Auto" HorizontalAlignment="Stretch">
	<Border BorderThickness="0,0,0,1" BorderBrush="#FFAAAAAA">
	<Grid Background="#dbdce1">
