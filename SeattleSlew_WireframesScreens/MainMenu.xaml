					</i:Interaction.Triggers>
				</MenuItem>
				<MenuItem Header="Previous" >
					<i:Interaction.Triggers>
						<i:EventTrigger EventName="Click">
							<pi:NavigateBackAction/>
						</i:EventTrigger>
					</i:Interaction.Triggers>
